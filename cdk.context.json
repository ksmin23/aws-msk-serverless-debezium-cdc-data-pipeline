{
  "vpc_name": "default",
  "db_cluster_name": "ecommerce",
  "msk_cluster_name": "debezium-cdc",
  "msk_connector_worker_configuration_name": "AuroraMySQLSource",
  "msk_connector_custom_plugin_name": "debezium-connector-mysql-v2-4-0",
  "msk_connector_name": "ecommerce-changes",
  "msk_connector_configuration": {
    "tasks.max": "1",
    "database.include.list": "ecommerce",
    "topic.prefix": "ecommerce-server",
    "topic.creation.default.partitions": "3",
    "topic.creation.default.replication.factor": "2",
    "include.schema.changes": "true",
    "schema.history.internal.kafka.topic": "schema-changes.ecommerce"
  },
  "firehose": {
    "buffering_hints": {
      "intervalInSeconds": 300,
      "sizeInMBs": 100
    },
    "topic_name": "ecommerce-server.ecommerce.orders"
  }
}
